{
  "name": "lolo-backend-scraping",
  "version": "1.0.0",
  "description": "scrapping microservice",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts --exec ts-node",
    "lint": "eslint",
    "production:build": "tsc && npm run production:copy-docs",
    "production:copy-docs": "npm run production:create-docs-dir && cp -R src/docs/* dist/docs",
    "production:create-docs-dir": "mkdir -p dist/docs",
    "production:start": "node dist/index.js",
    "migrations:generate": "sequelize-cli  migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:revert:all": "sequelize-cli db:migrate:undo:all",
    "seeders:generate": "sequelize-cli seed:generate --name",
    "seeders:run:all": "sequelize-cli db:seed:all",
    "seeders:run": "sequelize-cli db:seed --seed",
    "seeders:revert": "sequelize-cli db:seed:undo",
    "seeders:revert:all": "sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaron-0406/lolo-backend-scraping.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aaron-0406/lolo-backend-scraping/issues"
  },
  "homepage": "https://github.com/aaron-0406/lolo-backend-scraping#readme",
  "dependencies": {
    "@2captcha/captcha-solver": "^1.2.0",
    "@aws-sdk/client-dynamodb": "^3.245.0",
    "@aws-sdk/client-s3": "^3.245.0",
    "@hapi/boom": "^10.0.0",
    "@types/formidable": "^3.4.5",
    "@types/qrcode": "^1.5.5",
    "aws-sdk": "^2.1348.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "docx": "^7.8.2",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "formidable": "^3.5.1",
    "fs-extra": "^11.1.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.45",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "puppeteer": "^22.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "qrcode": "^1.5.3",
    "request": "^2.88.2",
    "sequelize": "^6.26.0",
    "speakeasy": "^2.0.0",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.12.13",
    "@types/node-cron": "^3.0.11",
